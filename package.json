{
  "name": "papr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dbpush:dev": "dotenv -e .env.dev -- prisma db push",
    "dbpush:prod": "dotenv -e .env.production.local -- prisma db push",
    "dbseed:dev": "dotenv -e .env.dev -- prisma db seed",
    "dbseed:prod": "dotenv -e .env.production.local -- prisma db seed",
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "test": "mocha"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} migrations/seed.ts",
    "reset": "npx prisma migrate reset --force && npx prisma db push && npx prisma db seed",
    "schema": "src/libs/prisma/schema.prisma"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.635.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@paypal/paypal-server-sdk": "^0.6.1",
    "@paypal/react-paypal-js": "^8.7.0",
    "@prisma/client": "^6.4.1",
    "@prisma/debug": "^6.4.1",
    "@rsuite/icons": "^1.0.3",
    "@tanstack/react-query": "^5.56.2",
    "@tanstack/react-query-devtools": "^5.59.15",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "bootstrap": "^5.3.3",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "gray-matter": "^4.0.3",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.0.3",
    "micro": "^10.0.1",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "next": "^15.1.2",
    "next-auth": "^4.24.11",
    "next-swagger-doc": "^0.4.1",
    "nodemailer": "^6.10.0",
    "papaparse": "^5.5.3",
    "react": "18.2.0",
    "react-bootstrap": "^2.10.7",
    "react-countdown": "^2.3.6",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-markdown": "^8.0.7",
    "react-phone-number-input": "^3.4.12",
    "react-player": "^2.16.0",
    "react-slick": "^0.29.0",
    "react-toastify": "^10.0.5",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^3.0.1",
    "resend": "^4.1.2",
    "rsuite": "^5.75.0",
    "sharp": "^0.31.1",
    "slugify": "^1.6.6",
    "swagger-ui-react": "^5.18.3",
    "uuidv4": "^6.2.13",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.14.9",
    "@types/papaparse": "^5.3.16",
    "chai": "^5.1.2",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5",
    "mocha": "^11.0.1",
    "nyc": "^17.1.0",
    "prisma": "^6.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4"
  }
}
